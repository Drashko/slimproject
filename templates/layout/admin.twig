    <main class="main" id="top">
        {% block sidebar %}
            {{  include('admin/partials/sidebar.twig') }}
        {% endblock %}
        {% block header %}
            {{  include('admin/partials/header.twig') }}
        {% endblock %}

        {% block content %}{% endblock %}

    </main>
    {% block javascripts %}
